package Stack_LinkedList;


public class LinkList {
	
	private Link first;
	
	// 생성자
	public LinkList() {
		// 처음 연결리스트를 아무런 인자없이 생성하면 빈 리스트가 생성된다.
		first = null;
	}
	
	// isEmpty
	public boolean isEmpty(){ return first == null; }
	
	// 삽입
	public void insert(int d1, double d2){
		Link link = new Link(d1, d2);
		link.nextLink = first;
		first = link;
	}
	
	// 삭제
	public Link delete() {
		// 정보 저장
		Link tmp = first;
		first = first.nextLink;
		// 파이썬과의 차이 : Garbage Collecotr. 파이썬은 delete를 직접 해주었지만 자바는 알아서.
		System.gc();
		return tmp;
		
	}

	// 출력

}
